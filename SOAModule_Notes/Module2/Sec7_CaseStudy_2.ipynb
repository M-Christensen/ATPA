   "source": [
    "visits_new = visits.groupby(['ID', 'year', 'type', 'reschedule'], \n",
    "                           as_index=False).size() \\\n",
    "                   .merge(visits.groupby(['ID', 'year'], as_index=False) \\\n",
    "                                .size() \\\n",
    "                                .sort_values('ID'), \n",
    "                          on=['ID', 'year'], \n",
    "                          how='inner') \\\n",
    "                   .assign(prop = lambda df: df.size_x/df.size_y, type_reschedule = lambda df: df.type+\"_\"+df.reschedule) \\\n",
    "                   .rename(columns={'size_y': 'visits'})  \\\n",
    "                   .drop(['type', 'reschedule', 'size_x'], axis=1) \\\n",
    "                   .pivot(index=['ID', 'year', 'visits'], columns = 'type_reschedule', values = 'prop') \\\n",
    "                   .replace(np.nan, 0).reset_index()\n",
    "\n",
    "visits_new.head()"
   ]
